<?xml version="1.0" encoding="utf-8"?>
<level-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:maxLevel="1">
        <shape android:shape="rectangle">
            <corners android:radius="12.0dip" />
            <gradient
                android:startColor="#fff42f4c"
                android:endColor="#fff42f4c"
                android:angle="180.0"
                android:centerX="0.15"
                android:centerColor="#fff42f4c" />
        </shape>
    </item>
    <item android:maxLevel="2">
        <shape android:shape="rectangle">
            <corners android:radius="12.0dip" />
            <gradient
                android:startColor="#fff42f4c"
                android:endColor="#42f42f4c"
                android:angle="180.0"
                android:centerX="0.15"
                android:centerColor="#aaf42f4c" />
        </shape>
    </item>
    <item android:maxLevel="3">
        <shape android:shape="rectangle">
            <corners android:radius="12.0dip" />
            <gradient
                android:startColor="#fff42f4c"
                android:endColor="#42f42f4c"
                android:angle="0.0"
                android:centerX="0.15"
                android:centerColor="#aaf42f4c" />
        </shape>
    </item>
</level-list>