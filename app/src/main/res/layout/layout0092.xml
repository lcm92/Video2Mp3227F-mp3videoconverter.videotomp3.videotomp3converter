<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical"
    android:background="@color/color0067"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.cardview.widget.CardView
        android:id="@id/id00b4"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="12.0dip"
        android:layout_marginBottom="4.0dip"
        android:layout_marginStart="12.0dip"
        android:layout_marginEnd="12.0dip"
        app:cardCornerRadius="8.0dip"
        app:cardElevation="2.0dip">

        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="vertical"
            android:id="@id/id0225"
            android:background="@color/color00c8"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@id/id032a"
                android:paddingTop="16.0dip"
                android:paddingBottom="16.0dip"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:paddingStart="16.0dip"
                android:paddingEnd="12.0dip">

                <TextView
                    android:textSize="16.0sp"
                    android:textColor="@color/color005f"
                    android:id="@id/id0411"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@id/id01a7"
                    android:fontFamily="sans-serif-medium" />

                <ImageView
                    android:id="@id/id01a7"
                    android:layout_width="32.0dip"
                    android:layout_height="32.0dip"
                    android:src="@drawable/drawable020b"
                    android:layout_centerVertical="true"
                    android:rotation="90.0"
                    android:layout_alignParentEnd="true" />
            </RelativeLayout>

            <LinearLayout
                android:orientation="vertical"
                android:id="@id/id0329"
                android:visibility="gone"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_margin="12.0dip">

                <TextView
                    android:textSize="14.0sp"
                    android:textColor="@color/color005f"
                    android:id="@id/id040f"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" />

                <ImageView
                    android:layout_gravity="center_horizontal"
                    android:id="@id/id01a5"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12.0dip"
                    android:scaleType="fitXY" />

                <com.airbnb.lottie.LottieAnimationView
                    android:id="@id/id025f"
                    android:visibility="gone"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12.0dip" />

                <TextView
                    android:textSize="14.0sp"
                    android:textColor="@color/color005f"
                    android:id="@id/id0410"
                    android:visibility="gone"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12.0dip"
                    android:layout_marginBottom="12.0dip" />

                <ImageView
                    android:layout_gravity="center_horizontal"
                    android:id="@id/id01a6"
                    android:visibility="gone"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:scaleType="fitXY" />
            </LinearLayout>
        </LinearLayout>
    </androidx.cardview.widget.CardView>
</LinearLayout>