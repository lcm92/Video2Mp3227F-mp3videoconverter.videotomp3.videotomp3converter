<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:background="@android:color/white"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <RelativeLayout
        android:id="@id/id0201"
        android:background="#ffffffff"
        android:layout_width="fill_parent"
        android:layout_height="?android:actionBarSize">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@id/id0086"
            android:background="@drawable/drawable007f"
            android:layout_width="@dimen/dimen029c"
            android:layout_height="fill_parent"
            android:scaleType="center"
            app:srcCompat="@drawable/drawable0250" />

        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginLeft="16.0dip"
            android:layout_toRightOf="@id/id0086">

            <androidx.appcompat.widget.AppCompatTextView
                android:textSize="20.0sp"
                android:textStyle="bold"
                android:textColor="#ff000000"
                android:gravity="center_vertical"
                android:id="@id/id03f7"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:minWidth="80.0dip"
                android:text="@string/string01f8"
                android:maxLines="1"
                android:singleLine="true"
                android:layout_weight="1.0" />

            <androidx.appcompat.widget.AppCompatImageView
                android:gravity="center"
                android:id="@id/id00a5"
                android:background="@drawable/drawable007f"
                android:layout_width="@dimen/dimen029c"
                android:layout_height="fill_parent"
                android:scaleType="center"
                app:srcCompat="@drawable/drawable019f" />

            <androidx.appcompat.widget.AppCompatTextView
                android:textSize="14.0sp"
                android:textColor="@color/color0066"
                android:id="@id/id036b"
                android:background="@drawable/drawable00de"
                android:paddingLeft="16.0dip"
                android:paddingTop="4.0dip"
                android:paddingRight="16.0dip"
                android:paddingBottom="4.0dip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/string0202"
                android:fontFamily="@string/string0117"
                android:layout_marginEnd="16.0dip" />
        </LinearLayout>

        <com.inshot.videotomp3.utils.widget.ClearEditText
            android:textSize="16.0sp"
            android:textColor="#ffffffff"
            android:textColorHint="#aaffffff"
            android:id="@id/id035b"
            android:visibility="gone"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginLeft="16.0dip"
            android:layout_marginRight="16.0dip"
            android:hint="@string/string01f5"
            android:drawableRight="@drawable/drawable017a"
            android:layout_toRightOf="@id/id0086"
            android:textCursorDrawable="@null"
            app:backgroundTint="#ddffffff" />
    </RelativeLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@id/id049f"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@id/id0201" />

    <ViewStub
        android:id="@id/id01fb"
        android:layout="@layout/layout00af"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@id/id0201" />

    <FrameLayout
        android:id="@id/id0064"
        android:background="#f5ffffff"
        android:visibility="gone"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@id/id0201">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@id/id0065"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" />
    </FrameLayout>
</RelativeLayout>