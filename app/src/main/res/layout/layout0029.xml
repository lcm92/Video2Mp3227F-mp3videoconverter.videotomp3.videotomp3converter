    <androidx.recyclerview.widget.RecyclerView
        android:id="@id/id030b"
        android:background="#ffffffff"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@id/id004b" />

    <RelativeLayout
        android:orientation="vertical"
        android:background="@drawable/drawable00ef"
        android:paddingTop="16.0dip"
        android:paddingBottom="20.0dip"
        android:layout_width="fill_parent"
        android:layout_height="218.0dip"
        android:layout_alignParentBottom="true">

        <androidx.appcompat.widget.AppCompatTextView
            android:textSize="18.0sp"
            android:textColor="@color/color005f"
            android:id="@id/id0464"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/string0228"
            android:fontFamily="@string/string0117"
            android:layout_marginStart="20.0dip" />

        <com.inshot.videotomp3.view.SpeedSeekBar
            android:id="@id/id0391"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="26.0dip"
            android:progress="5"
            android:progressDrawable="@android:color/transparent"
            android:thumb="@drawable/drawable00ea"
            android:thumbOffset="8.0dip"
            android:layout_below="@id/id0464"
            android:paddingStart="22.0dip"
            android:paddingEnd="22.0dip"
            app:layout_constraintEnd_toEndOf="id02c7"
            app:layout_constraintStart_toStartOf="id02c7"
            app:layout_constraintTop_toBottomOf="@id/id0464"
            app:max="35"
            app:min="5" />

        <androidx.appcompat.widget.AppCompatTextView
            android:textSize="14.0sp"
            android:textColor="@color/color00c8"
            android:gravity="center"
            android:id="@id/id00aa"
            android:background="@drawable/drawable00da"
            android:layout_width="fill_parent"
            android:layout_height="40.0dip"
            android:layout_marginTop="24.0dip"
            android:text="@string/string01f0"
            android:layout_below="@id/id0391"
            android:textAllCaps="true"
            android:fontFamily="@string/string0117"
            android:layout_marginStart="24.0dip"
            android:layout_marginEnd="24.0dip" />
    </RelativeLayout>
</RelativeLayout>