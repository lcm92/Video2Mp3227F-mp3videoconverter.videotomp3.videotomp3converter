<?xml version="1.0" encoding="utf-8"?>
<FrameLayout android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextureView
        android:layout_gravity="center"
        android:id="@id/id02e4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <LinearLayout
        android:layout_gravity="bottom"
        android:orientation="horizontal"
        android:id="@id/id02d8"
        android:background="#61000000"
        android:visibility="gone"
        android:layout_width="fill_parent"
        android:layout_height="48.0dip">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@id/id02d6"
            android:background="@drawable/drawable007e"
            android:padding="12.0dip"
            android:layout_width="48.0dip"
            android:layout_height="48.0dip"
            android:layout_marginLeft="4.0dip"
            android:tint="#ffffffff"
            app:srcCompat="@drawable/drawable01e8" />

        <SeekBar
            android:layout_gravity="center_vertical"
            android:id="@id/id02df"
            android:paddingLeft="14.0dip"
            android:paddingRight="14.0dip"
            android:layout_width="0.0dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8.0dip"
            android:maxHeight="2.0dip"
            android:progressDrawable="@drawable/drawable02ae"
            android:minHeight="2.0dip"
            android:layout_weight="1.0" />

        <TextView
            android:textSize="12.0sp"
            android:textColor="#ffffffff"
            android:layout_gravity="center_vertical"
            android:id="@id/id02de"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8.0dip"
            android:layout_marginRight="8.0dip"
            android:text="00:00/00:00" />
    </LinearLayout>

    <RelativeLayout
        android:layout_gravity="center"
        android:id="@id/id0262"
        android:visibility="gone"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@id/id02da"
            android:background="@drawable/drawable011d"
            android:layout_width="40.0dip"
            android:layout_height="40.0dip"
            android:scaleType="center"
            android:layout_centerInParent="true"
            app:srcCompat="@drawable/drawable01e8" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@id/id0312"
            android:background="@drawable/drawable011d"
            android:layout_width="40.0dip"
            android:layout_height="40.0dip"
            android:layout_marginRight="24.0dip"
            android:scaleType="center"
            android:layout_toLeftOf="@id/id02da"
            android:layout_centerVertical="true"
            app:srcCompat="@drawable/drawable01f2" />
    </RelativeLayout>
</FrameLayout>